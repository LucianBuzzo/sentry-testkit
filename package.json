{
  "name": "sentry-testkit",
  "version": "2.0.0",
  "description": "Sentry test kit module to enable reports to sentry in tests without really sending the report",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js --out-dir dist",
    "test": "npm run-script build && jest --config jest.config.json",
    "prepublish": "npm run-script build",
    "docs": "jsdoc2md index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/sentry-testkit.git"
  },
  "keywords": [
    "raven-test-kit",
    "raven",
    "sentry",
    "sentry-io",
    "raven-js",
    "raven-node",
    "sentry-testkit",
    "error-reporting",
    "error-monitoring",
    "sentry-error-handler",
    "sentry-plugin",
    "sentry-client"
  ],
  "author": "Ziv Levy (zivl@wix.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/sentry-testkit/issues"
  },
  "homepage": "https://github.com/wix/sentry-testkit#readme",
  "devDependencies": {
    "@sentry/browser": "^4.4.2",
    "@sentry/node": "^4.4.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^23.6.0",
    "wait-for-expect": "^1.1.0"
  },
  "peerDependencies": {
    "@sentry/browser": "^4.4.2",
    "@sentry/node": "^4.4.2"
  }
}
