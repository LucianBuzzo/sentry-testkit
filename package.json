{
  "name": "raven-testkit",
  "version": "1.3.18",
  "description": "Raven test kit module to enable reports to sentry in tests without really sending the report",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js --out-dir dist",
    "test": "npm run-script build && jest --config jest.config.json",
    "release": "wnpm-release --no-shrinkwrap",
    "prepublish": "npm run-script build",
    "docs": "jsdoc2md index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/raven-testkit.git"
  },
  "keywords": [
    "raven",
    "testkit",
    "test kit",
    "raven test kit",
    "raven test",
    "sentry",
    "sentry.io",
    "sentry test kit"
  ],
  "author": "Ziv Levy (zivl@wix.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/raven-testkit/issues"
  },
  "homepage": "https://github.com/wix/raven-testkit#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^23.6.0",
    "jsdoc-to-markdown": "^3.0.4",
    "raven-js": "^3.18.1",
    "wnpm-ci": "*"
  },
  "peerDependencies": {
    "raven-js": "^3.18.1"
  }
}
